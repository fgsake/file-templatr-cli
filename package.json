{
  "name": "@fgsake/file-templatr-cli",
  "private": "true",
  "version": "1.0.0",
  "description": "Utility to create files / folders from a pre-defined template within your project directory.",
  "main": "./bin/index.js",
  "author": "Eric Heaton <eric@fgsake.org>",
  "license": "MIT",
  "bin": {
    "file-templatr": "./bin/index.js"
  },
  "scripts": {
    "start": "nodemon ./src/index.ts -- -t react-ts-component -p ./test-output -f --watch --debug \"My Test Component\"",
    "create": "yarn run build && yarn run test",
    "build": "tsc --noEmit false -p .",
    "local": "yarn i -g && create-component",
    "refresh": "rm -rf ./node_modules ./yarn.lock && yarn install",
    "prepack": "yarn build",
    "postinstall": "yarn build"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.12",
    "app-root-path": "^3.1.0",
    "chalk": "^5.0.1",
    "change-case": "^4.1.2",
    "commander": "^9.4.0",
    "dotenv": "^16.0.1",
    "find-root": "^1.1.0",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.19",
    "optional-require": "^1.1.8",
    "path": "^0.12.7",
    "totalist": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
